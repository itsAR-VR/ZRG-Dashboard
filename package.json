{
  "name": "zrg-dashboard",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "prisma generate && next build --webpack",
    "dev": "next dev",
    "lint": "eslint .",
    "test": "node --import tsx scripts/test-orchestrator.ts",
    "test:ai-drafts": "node --import tsx scripts/test-ai-drafts.ts",
    "test:ai-replay": "node --require ./scripts/server-only-mock.cjs --import tsx scripts/live-ai-replay.ts",
    "test:ai-replay:sample": "npm run test:ai-replay -- --limit 2 --concurrency 1",
    "test:coverage": "node --import tsx scripts/test-coverage-orchestrator.ts",
    "typecheck": "tsc --noEmit",
    "start": "next start",
    "postinstall": "prisma generate",
    "migrate:appointments": "tsx scripts/migrate-appointments.ts",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "logs:check": "node --import tsx scripts/logs/assert-known-errors.ts",
    "inngest:dev": "npx inngest-cli@latest dev -u http://localhost:3000/api/inngest"
  },
